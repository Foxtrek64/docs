{
  "build": {
    "content": [
      {
        "files": [
		  "**/*.md",
		  "**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
		  "**/*.jpg",
		  "**/*.gif",
		  "**/*.pdf",
		  "**/*.pptx"
        ]
      }
    ],
	"overwrite": [],
	"externalReference": [],
	"globalMetadata": {
      "_appTitle": "LuzFaltex Documentation",
      "_appFooter": "LuzFaltex (c) 2014-2018",
      "_enableSearch": true,
	  "titleSuffix": "LuzFaltex Docs",
	  "feedback_system": "GitHub",
	  "feedback_github_repo": "LuzFaltex/docs",
	  "author": "Foxtrek_64"
	  },
	"fileMetadata": {},
	"template": [
	  "default",
	  "_template/light-dark-theme",
	  "_template/last-modified",
	  "_template/description-generator"
	],
    "dest": "_site",
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}